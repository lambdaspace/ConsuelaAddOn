{

  "manifest_version": 2,
  "name": "Lambda Hackerspace",
  "version": "0.7.0",

  "description": "Lambda space's information system. This WebExtension informs you about the status of the hackerspace and the upcoming events",

  "browser_action": {
  "default_icon": "icons/c_48x48.png",
  "default_title": "Lambda Hackerspace status!"
  },

  "background": {
    "scripts": ["MinisterIn.js", "eventData.js"]
  },

  "permissions": [
    "*://*.lambdaspace.gr/*"
  ],

  "applications": {
    "gecko": {
      "id": "info@lambdaspace.gr",
      "strict_min_version": "45.0"
    }
  }

}
